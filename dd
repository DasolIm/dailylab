    for(int i=1;i<=user;i++){
        for(int j=0;j<user-i;j++) printf(" ");
        for(int j=1;j<=i;j++) printf("%d",3*j);
        for(int j=i-1;j>=1;j--) printf("%d",3*j);
        printf("\n");
    }
    for(int i=user-1;i>=1;i--){
        for(int j=0;j<user-i;j++) printf(" ");
        for(int j=1;j<=i;j++) printf("%d",3*j);
        for(int j=i-1;j>=1;j--) printf("%d",3*j);
        printf("\n");
    }
    return 0;
}


