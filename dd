#include <stdio.h>

int main() {
    int user = 0;
    scanf("%d", &user);

    int i = 0, j = 0;
    
    for (i=1; i<=user; i++) {
        for (j=user-i; j>0; j--) {
            printf(" ");
        }
        for (j=1; j<=i*2-1; j++) {
            printf("%d", i);
        }
        printf("\n");
    }

    for (i=user-1; i>0; i--) {
        for (j=user-i; j>0; j--) {
            printf(" ");
        }
        for (j=1; j<=i*2-1; j++) {
            printf("%d", i);
        }
        printf("\n");
    }
}




