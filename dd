#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= i; j++) printf("*");
        for (j = 1; j <= 2 * (user - i); j++) printf(" ");
        for (j = 1; j <= i; j++) printf("*");
        printf("\n");
    }
    for (i = user; i >= 1; i--) {
        for (j = 1; j <= i; j++) printf("*");
        for (j = 1; j <= 2 * (user - i); j++) printf(" ");
        for (j = 1; j <= i; j++) printf("*");
        printf("\n");
    }
    return 0;
}



