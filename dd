#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= user; j++) {
            if ((i + j) % 2 == 0) printf("1");
            else printf("0");
        }
        printf("\n");
    }
    return 0;
}





