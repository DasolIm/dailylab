#include <stdio.h>
int main(){
    int user; scanf("%d",&user);
    for(int i=1;i<=user;i++){
        for(int j=0;j<user-i;j++) printf(" ");
        for(int j=0;j<i;j++) printf("%c",'a'+j);
        for(int j=i-2;j>=0;j--) printf("%c",'a'+j);
        printf("\n");
    }
    for(int i=user-1;i>=1;i--){
        for(int j=0;j<user-i;j++) printf(" ");
        for(int j=0;j<i;j++) printf("%c",'a'+j);
        for(int j=i-2;j>=0;j--) printf("%c",'a'+j);
        printf("\n");
    }
    return 0;
}


