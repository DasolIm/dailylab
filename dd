#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = user; i >= 1; i--) {
        for (j = 0; j < user - i; j++) printf(" ");
        for (j = 0; j < 2 * i - 1; j++) printf("%d", i);
        printf("\n");
    }
    for (i = 2; i <= user; i++) {
        for (j = 0; j < user - i; j++) printf(" ");
        for (j = 0; j < 2 * i - 1; j++) printf("%d", i);
        printf("\n");
    }
    return 0;
}



