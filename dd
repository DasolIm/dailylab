#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= user - i; j++) printf(" ");
        for (j = 1; j <= 2 * i - 1; j++) {
            if (j == 1 || j == 2 * i - 1) printf("*");
            else printf(" ");
        }
        printf("\n");
    }
    for (i = user - 1; i >= 1; i--) {
        for (j = 1; j <= user - i; j++) printf(" ");
        for (j = 1; j <= 2 * i - 1; j++) {
            if (j == 1 || j == 2 * i - 1) printf("*");
            else printf(" ");
        }
        printf("\n");
    }
    return 0;
}



   
