#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int mid = (user + 1) / 2;
    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= user; j++) {
            if (i == mid || j == mid) printf("*");
            else printf(" ");
        }
        printf("\n");
    }
    return 0;
}



