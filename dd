#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= user; j++) {
            if (j == i || j == (user - i + 1))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}




