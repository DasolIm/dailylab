#include <stdio.h>

int main() {
    int user;
    scanf("%d", &user);

    int i, j;
    for (i = 1; i <= user; i++) {
        for (j = 1; j <= i; j++) {
            if (j == 1 || i == user || j == i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}




